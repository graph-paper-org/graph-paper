{
  "name": "@graph-paper/graph-paper",
  "version": "0.0.1",
  "description": "Reusable Svelte components for building data visualizations & data products",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "format": "prettier --plugin-search-dir=. --write .",
    "format:check": "prettier --plugin-search-dir=. --check .",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint .",
    "lint:shell": "shellcheck all create",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "deploy-storybook": "storybook-to-ghpages",
    "test": "npm-run-all format:check lint test:jest",
    "test:jest": "jest test --no-cache",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.14.0",
    "@storybook/addon-knobs": "^6.0.12",
    "@storybook/addon-storysource": "^6.0.10",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/svelte": "^6.0.12",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@testing-library/jest-dom": "^5.14.0",
    "@testing-library/svelte": "^3.0.0",
    "@types/jest": "^27.4.0",
    "@types/testing-library__jest-dom": "^5.14.0",
    "babel-jest": "^27.0.0",
    "babel-loader": "^8.1.0",
    "d3-array": "^3.1.1",
    "d3-dsv": "^3.0.1",
    "d3-format": "^3.1.0",
    "d3-random": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "d3-shape": "^3.1.0",
    "d3-time": "^3.0.0",
    "d3-time-format": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "jest": "^27.0.0",
    "jest-transform-svelte": "^2.1.1",
    "micromatch": "^4.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "shellcheck": "^1.1.0",
    "svelte": "^3.44.0",
    "svelte-jester": "^2.0.1",
    "svelte-loader": "^2.13.6",
    "svelte2tsx": "^0.5.4",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
