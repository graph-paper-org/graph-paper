{
  "name": "@graph-paper/project",
  "version": "0.0.0",
  "description": "Reusable Svelte components for building data visualizations & data products",
  "main": "index.js",
  "scripts": {
    "format": "prettier --plugin-search-dir=. --write .",
    "format:check": "prettier --plugin-search-dir=. --check .",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint .",
    "lint:shell": "shellcheck all create",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "deploy-storybook": "storybook-to-ghpages",
    "test": "npm-run-all format:check lint test:jest",
    "test:jest": "jest test --no-cache"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "7.11.0",
    "@storybook/addon-knobs": "^6.0.12",
    "@storybook/addon-storysource": "^6.0.10",
    "@storybook/storybook-deployer": "^2.8.6",
    "@storybook/svelte": "^6.0.12",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "d3-dsv": "^1.2.0",
    "d3-random": "^2.1.0",
    "d3-scale-chromatic": "^1.5.0",
    "d3-time": "^2.0.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^24.1.8",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-svelte3": "^2.7.3",
    "jest": "^26.0.1",
    "jest-transform-svelte": "^2.1.1",
    "lerna": "^3.20.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "prettier-plugin-svelte": "^1.1.0",
    "shellcheck": "^0.4.2",
    "svelte": "^3.20.1",
    "svelte-loader": "^2.13.6"
  }
}
